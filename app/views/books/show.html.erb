<h1>Book detail</h1>

<table>
  <tbody>
    <% @books.each do |book| %>
      <tr>
      <td>画像<br>名前</td>
      <td><%= book.title %></td>
      <td><%= book.body %></td>
      <td><%= link_to "Edit", edit_book_path(book.id) %></td>
      <td>Destroy</td>
    </tr>
    <% end %>
  </tbody>
</table>